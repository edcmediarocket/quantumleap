
{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": false, 
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2021",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "compileOnSave": true,
  "include": [
    "src", 
    "index.ts",
    "investmentCoachAgent.ts",
    "push.ts",
    "firestore.ts"
  ],
  "exclude": [
    "node_modules",
    "lib"
  ]
}
